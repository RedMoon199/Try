<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/lblResult"
        android:gravity="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="22dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/lblRetry"
        tools:layout_editor_absoluteX="299dp"
        tools:layout_editor_absoluteY="665dp" />

    <TextView
        android:id="@+id/nbPersonnes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="nbPersones"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="95dp" />

    <TextView
        android:id="@+id/nbMortRes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="nbMort"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="225dp" />

    <TextView
        android:id="@+id/nbContamine"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="nbContamine"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="185dp" />
    <TextView
        android:id="@+id/nbSain"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="nbSain"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="140dp" />

</androidx.constraintlayout.widget.ConstraintLayout>